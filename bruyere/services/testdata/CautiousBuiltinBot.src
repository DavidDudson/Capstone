package test.nz.ac.massey.cs.ig.core.services.api;

import java.util.List;
import nz.ac.massey.cs.ig.games.primegame.*;

/**
 * Built-in bot, plays a random number.
 * @author jens dietrich
 */
public class CautiousBot extends PGBot {

    public CautiousBot(String botId) {
        super(botId);
    }

	@Override
	public Integer nextMove(List<Integer> game) {
		List<Integer> numbers = game;
		assert numbers.size()>0;
		
		return numbers.get(0);
	}


}
